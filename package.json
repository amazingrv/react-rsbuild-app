{
  "name": "react-rsbuild-app",
  "version": "1.0.0",
  "description": "react app template with rsbuild, eslint and prettier",
  "author": "amazingrv",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_CONFIG_ENV=e0 rsbuild dev",
    "build": "cross-env NODE_CONFIG_ENV=e0 rsbuild build",
    "analyze": "cross-env NODE_CONFIG_ENV=e0 RSDOCTOR=true rsbuild build",
    "test": "rstest",
    "test:watch": "rstest --watch"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@rsbuild/core": "^1.6.15",
    "@rsbuild/plugin-eslint": "^1.2.0",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rsdoctor/rspack-plugin": "^1.4.0",
    "@rstest/core": "^0.7.7",
    "@rstest/coverage-istanbul": "^0.1.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "autoprefixer": "^10.4.23",
    "config": "^4.1.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^5.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "liquidjs": "^10.24.0",
    "prettier": "^3.7.4"
  }
}
